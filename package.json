{
  "name": "my-example-project",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "ng build",
    "e2e": "ng e2e",
    "format": "prettier --write \"{apps,libs}/**/*.ts\"",
    "format:check": "prettier --list-different \"{apps,libs}/**/*.ts\"",
    "lint": "ng lint",
    "ng": "ng",
    "package:apps": "bb-ang package-apps",
    "package:libs": "bb-ang package-libs",
    "start": "ng serve",
    "test": "ng test",
    "create-translations": "ng xi18n && xliffmerge"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.3",
    "@angular/common": "6.0.3",
    "@angular/compiler": "6.0.3",
    "@angular/core": "6.0.3",
    "@angular/forms": "6.0.3",
    "@angular/platform-browser": "6.0.3",
    "@angular/platform-browser-dynamic": "6.0.3",
    "@angular/router": "6.0.3",
    "@backbase/data-ang": "^3.0.1",
    "@backbase/foundation-ang": "^3.7.0",
    "@backbase/reference-data-http-ang": "^1.0.0-pr.23",
    "@backbase/retail-ang": "^3.0.1",
    "@backbase/wealth-ang": "^3.0.1",
    "@bb-cli/bb-config": "^2.1.0",
    "@ng-bootstrap/ng-bootstrap": "2.0.0",
    "core-js": "^2.5.4",
    "npm": "^6.0.1",
    "rxjs": "6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "6.0.3",
    "@angular-devkit/build-angular": "~0.6.1",
    "typescript": "2.7.2",
    "@angular/cli": "6.0.3",
    "@angular/language-service": "6.0.3",
    "@bb-cli/bb-ang": "^2.0.1-pr.22",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "0.3.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "1.10.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1"
  },
  "xliffmergeOptions": {
    "srcDir": "apps/my-example-app/src",
    "genDir": "apps/my-example-app/src/locale",
    "i18nFile": "messages.xlf",
    "i18nBaseFile": "messages",
    "i18nFormat": "xlf",
    "encoding": "UTF-8",
    "defaultLanguage": "en",
    "languages": [
      "en-US",
      "es-AR"
    ],
    "removeUnusedIds": true,
    "supportNgxTranslate": false,
    "ngxTranslateExtractionPattern": "@@|ngx-translate",
    "useSourceAsTarget": true,
    "targetPraefix": "",
    "targetSuffix": "",
    "beautifyOutput": false,
    "allowIdChange": false,
    "autotranslate": false,
    "apikey": "",
    "apikeyfile": "",
    "verbose": false,
    "quiet": false
  }
}
